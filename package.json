{
  "name": "react-redux-ssr-cognizant",
  "version": "1.0.0",
  "description": "A Repo to make the simplest React-Redux-Server-Side-Rendering async app. A Scaffolding that is aimed at helping devs to set up a basic setup for a react-redux app.",
  "main": "src/server/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev-server": "cross-env BABEL_ENV=server NODE_ENV=development nodemon ./src/server/index.js --exec babel-node --ignore dist/ -e js,scss,css",
    "dev-client": "cross-env NODE_ENV=development webpack --config ./webpack.config.dev.js",
    "dev": "concurrently \"cross-env NODE_ENV=development npm run dev-server\" \"cross-env NODE_ENV=development npm run dev-client\"",
    "prod-server": "cross-env BABEL_ENV=server NODE_ENV=production babel src --out-dir ./dist",
    "prod-client": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod.js",
    "build": "npm run clean && npm run prod-server && npm run prod-client",
    "start": "cross-env NODE_ENV=production node ./dist/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shashiuilab/react-redux-ssr-boilerplate.git"
  },
  "keywords": [
    "React",
    "Redux",
    "SSR",
    "Server",
    "Side",
    "Rendering",
    "Boilerplate"
  ],
  "author": "Shashidhar Reddy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shashiuilab/react-redux-ssr-boilerplate/issues"
  },
  "homepage": "https://github.com/shashiuilab/react-redux-ssr-boilerplate#readme",
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@babel/runtime-corejs3": "^7.14.0",
    "core-js": "^3.12.1",
    "express": "^4.17.1",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.7",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0"
  }
}
